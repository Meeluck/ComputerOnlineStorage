@model ComputerOnlineStorage.Models.CountryProduce

@{
    ViewBag.Title = "Изменение страны производителя";
}

<h2>Изменение названий производителей</h2>
@using (Html.BeginForm())
{
    <fieldset>
        @Html.HiddenFor(m =>m.CountryId)
        <p>
            @Html.LabelFor(m => m.CountryName, "Название страны производителя")
            <br />
            @Html.EditorFor(m => m.CountryName)
        </p>
    </fieldset>
    <input type="submit" value="изменить">
}